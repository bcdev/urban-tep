<?xml version="1.0" encoding="UTF-8" standalone="yes" ?> <wps:Execute service="WPS"
             version="1.0.0"
             xmlns:wps="http://www.opengis.net/wps/1.0.0"
             xmlns:ows="http://www.opengis.net/ows/1.1"
             xmlns:xlink="http://www.w3.org/1999/xlink">
<!--
  <ows:Identifier>L2Plus</ows:Identifier>
-->
  <ows:Identifier>L2</ows:Identifier>
  <wps:DataInputs>
    <wps:Input>
      <ows:Identifier>productionName</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>Urban-TEP Idepix ${year} ${month}</wps:LiteralData>
      </wps:Data>
    </wps:Input>

    <wps:Input>
      <ows:Identifier>processorBundleName</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>s2ac</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>processorBundleVersion</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>1.3</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>processorName</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>Idepix.Sentinel2</wps:LiteralData>
      </wps:Data>
    </wps:Input>

    <wps:Input>
      <ows:Identifier>inputPath</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>${input}/${yyyy}/${MM}/${dd}/S2.*T23KLP.zip,/calvalus/eodata/S2_L1C/granules-urban/${yyyy}/${MM}/${dd}/S2.*T23KLQ.zip</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>inputPath</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>${input}/${yyyy}/${MM}/${dd}/S2.*T14QMG.zip,/calvalus/eodata/S2_L1C/granules-urban/${yyyy}/${MM}/${dd}/S2.*T14QNG.zip</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>inputPath</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>${input}/${yyyy}/${MM}/${dd}/S2.*T31NEH.zip</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>inputPath</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>${input}/${yyyy}/${MM}/${dd}/S2.*T48PXS.zip,/calvalus/eodata/S2_L1C/granules-urban/${yyyy}/${MM}/${dd}/S2.*T48PXT.zip,/calvalus/eodata/S2_L1C/granules-urban/${yyyy}/${MM}/${dd}/S2.*T48PYS.zip,/calvalus/eodata/S2_L1C/granules-urban/${yyyy}/${MM}/${dd}/S2.*T48PYT.zip</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>inputPath</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>${input}/${yyyy}/${MM}/${dd}/S2.*T43RGM.zip,/calvalus/eodata/S2_L1C/granules-urban/${yyyy}/${MM}/${dd}/S2.*T43RFM.zip</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>inputPath</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>${input}-urban/${yyyy}/${MM}/${dd}/S2.*T36RUU.zip</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>inputPath</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>${input}/${yyyy}/${MM}/${dd}/S2.*T50TMK.zip,/calvalus/eodata/S2_L1C/granules-urban/${yyyy}/${MM}/${dd}/S2.*T50TMJ.zip,/calvalus/eodata/S2_L1C/granules-urban/${yyyy}/${MM}/${dd}/S2.*T50SMJ.zip</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>inputPath</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>/calvalus/eodata/S2_L1C/granules-africa/${yyyy}/${MM}/${dd}/S2.*T36NVF.zip</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>calvalus.input.format</ows:Identifier>
      <wps:Data><wps:LiteralData>CALVALUS-SENTINEL-2-MSI-20M</wps:LiteralData></wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>minDate</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>${start}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>maxDate</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>${stop}</wps:LiteralData>
      </wps:Data>
    </wps:Input>

    <wps:Input>
      <ows:Identifier>calvalus.output.dir</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>${output}/${year}/${month}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
<!--
    <wps:Input>
        <ows:Identifier>outputFormat</ows:Identifier>
        <wps:Data><wps:LiteralData>NetCDF4</wps:LiteralData></wps:Data>
    </wps:Input>
-->
    <wps:Input>
      <ows:Identifier>calvalus.hadoop.mapred.map.max.attempts</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>2</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>calvalus.hadoop.mapreduce.map.java.opts</ows:Identifier>
      <wps:Data><wps:LiteralData>-Djava.awt.headless=true -Xmx2048M</wps:LiteralData></wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>calvalus.hadoop.mapreduce.map.memory.mb</ows:Identifier>
      <wps:Data><wps:LiteralData>2560</wps:LiteralData></wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>calvalus.hadoop.mapreduce.map.failures.maxpercent</ows:Identifier>
      <wps:Data><wps:LiteralData>10</wps:LiteralData></wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>calvalus.hadoop.mapreduce.task.timeout</ows:Identifier>
      <wps:Data><wps:LiteralData>1800000</wps:LiteralData></wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>calvalus.hadoop.mapreduce.job.queuename</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>tep</wps:LiteralData>
      </wps:Data>
    </wps:Input>
  </wps:DataInputs>
</wps:Execute>
